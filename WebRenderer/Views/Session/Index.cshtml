@model IEnumerable<Core.Models.Session>

<h1>Sessions</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>File Name</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var session in Model)
        {
            <tr>
                <td>@session.Id</td>
                <td>@session.FileName</td>
                <!-- Add more columns as needed -->
            </tr>
        }
    </tbody>
</table>
